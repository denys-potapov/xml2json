ntalNonqualRetirePlan": "false", "EquityBasedCompArrangement": "false", "CompBasedOnRevenueOfFilingOrg": "false", "CompBasedOnRevenueRelatedOrgs": "false", "CompBasedNetEarningsFilingOrg": "false", "CompBasedNetEarningsRelateOrgs": "false", "AnyNonFixedPayments": "false", "InitialContractException": "false"}, "IRS990ScheduleO": {"GeneralExplanation": [{"Identifier": "ORGANIZATION'S MISSION", "ReturnReference": "FORM 990 - ORGANIZATION'S MISSION", "Explanation": "PENOBSCOT COMMUNITY HEALTH CARE IS A NON-PROFIT ORGANIZATION GOVERNED BY A BOARD OF COMMUNITY VOLUNTEERS, INCORPORATED IN 1997. PCHC PROVIDES FAMILY MEDICINE, PEDIATRICS, GERIATRICS, NURSING HOME CARE, PSYCHIATRY, MENTAL HEALTH COUNSELING, DENTAL CARE, PHARMACY, SPECIALIST SERVICES, PHYSICAL THERAPY, LAB, X-RAY, SPEECH AND AUDIOLOGY, HOMELESS SERVICES/SHELTER AND EASTERN MAINE AIDS NETWORK. PCHC IS DRIVEN BY ITS MISSION-\"WE PROVIDE ACCESS TO PATIENT-CENTERED, HIGH QUALITY, COMPREHENSIVE, INTEGRATED HEALTH CARE SERVICES, REGARDLESS OF THE ABILITY TO PAY.\""}, {"Identifier": "ORGANIZATION'S PROCESS USED TO REVIEW FORM 990", "ReturnReference": "FORM 990, PAGE 6, PART VI, LINE 11B", "Explanation": "THE FINANCE COMMITTEE, A SUBCOMMITTEE OF THE BOARD OF DIRECTORS, ALONG WITH THE CEO AND CFO,REVIEW AND APPROVE FORM 990. EACH MEMBER OF THE FULL BOARD OF DIRECTORS RECEIVE A COPY PRIOR TO FILING."}, {"Identifier": "ENFORCEMENT OF CONFLICTS POLICY", "ReturnReference": "FORM 990, PAGE 6, PART VI, LINE 12C", "Explanation": "AN ANNUAL CONFLICT OF INTEREST IS FILED WITH THE PRESIDENT."}, {"Identifier": "COMPENSATION PROCESS FOR TOP OFFICIAL", "ReturnReference": "FORM 990, PAGE 6, PART VI, LINE 15A", "Explanation": "THE CEO'S SALARY IS SET BY THE BOARD OF DIRECTORS AND THE BOARD USES THEIR KNOWLEDGE OF OTHER SIMILAR POSITIONS IN THE LOCAL AREA PLUS THEY REVIEW THE RESULTS OF THE NATIONAL ASSOCIATION OF COMMUNITY HEALTH CENTER'S ANNUAL NATIONAL SALARY SURVEY OF ALL 1200 FEDERALLY QUALIFIED HEALTH CENTERS AND LOOK TO SIMILAR SIZE FQHCS FOR COMPARISON."}, {"Identifier": "COMPENSATION PROCESS FOR OFFICERS", "ReturnReference": "FORM 990, PAGE 6, PART VI, LINE 15B", "Explanation": "THE SALARIES FOR OTHER KEY EMPLOYEES IS DECIDED BY THE CEO USING THE SAME DATA SOURCES (LOCAL KNOWLEDGE AND THE NATIONAL ANNUAL SURVEY)."}, {"Identifier": "GOVERNING DOCUMENTS DISCLOSURE EXPLANATION", "ReturnReference": "FORM 990, PAGE 6, PART VI, LINE 19", "Explanation": "THE GOVERNING DOCUMENTS, CONFLICT OF INTEREST POLICY AND FINANCIAL STATEMENTS ARE MAINTAINED IN THE OFFICE AND ARE AVAILABLE TO THE PUBLIC DURING STANDARD BUSINESS HOURS."}]}, "IRS990@documentId": "RetDoc4", "IRS990@referenceDocumentId": "RetDoc6", "IRS990ScheduleA@documentId": "RetDoc2", "IRS990ScheduleB@documentId": "RetDoc1", "IRS990ScheduleD@documentId": "RetDoc3", "IRS990ScheduleJ@documentId": "RetDoc5", "IRS990ScheduleO@documentId": "RetDoc6"}, "ReturnHeader@binaryAttachmentCount": "0", "ReturnData@documentCount": "6"}, "Return@returnVersion": "2010v3.2"}