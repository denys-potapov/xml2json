oardOrCommitteeApproval": "X", "SeverancePayment": "0", "SupplementalNonqualRetirePlan": "0", "EquityBasedCompArrangement": "0", "CompBasedOnRevenueOfFilingOrg": "0", "CompBasedOnRevenueRelatedOrgs": "0", "CompBasedNetEarningsFilingOrg": "0", "CompBasedNetEarningsRelateOrgs": "0", "AnyNonFixedPayments": "0", "InitialContractException": "0"}, "IRS990ScheduleO": {"GeneralExplanation": [{"Identifier": "FORM 990, PART VI, SECTION A, LINE 2", "Explanation": "MARY M LOOKER AND ROBERT L LOOKER ARE MOTHER AND SON AND VOTING MEMBERS OF THE SCHOOL'S BOARD OF TRUSTEES."}, {"Identifier": "FORM 990, PART VI, SECTION A, LINE 8B", "Explanation": "COMMITTEE ACTIONS AND RECOMMENDATIONS ARE COMMUNICATED TO THE BOARD OF TRUSTEES AT THEIR MEETINGS."}, {"Identifier": "FORM 990, PART VI, SECTION B, LINE 11", "Explanation": "A DRAFT COPY OF THE FORM 990 IS DISTRIBUTED TO THE PRESIDENT, TREASURER AND EXECUTIVE DIRECTOR PRIOR TO FILING FOR THE PURPOSE OF DISCUSSION AND RECOMMENDED CHANGES."}, {"ReturnReference": "FORM 990, PART VI, SECTION B, LINE 12", "Explanation": "CONFLICT OF INTEREST POLICY COVERS ALL TRUSTEES, OFFICERS AND EMPLOYEES. THE BOARD APPROVED REQUIRING ALL TRUSTEES, OFFICERS AND EMPLOYEES TO ANNUALLY FILL OUT A CONFLICT OF INTEREST DISCLOSURE FORM, WHICH WILL ASK ABOUT INTERESTS THAT COULD GIVE RISE TO CONFLICTS. THE SCHOOL USE THE INFORMATION ON THE DISCLOSURE FORMS TO MONITOR FOR POTENTIAL CONFLICTS. CONFLICTS INVOLVING EMOLOYEES, OTHER THAN EXECUTIVE DIRECTOR, ARE REVIEWED AND RESOLVED BY THE EXECUTIVE DIRECTOR. CONFLICTS INVOLVING TRUSTEES, OFFICERS, OR THE EXECUTIVE DIRECTOR, ARE REVIEWED AND RESOLVED BY THE BOARD. NO ONE WITH A CONFLICT IS ALLOWED TO PARTICIPATE IN ANY AFFECTED DECEISION."}, {"ReturnReference": "FORM 990, PART VI, SECTION B, LINE 15", "Explanation": "SALARIES ARE NEGOTIATED BY BOARD PRESIDENT AND APPROVED BY THE EXECUTIVE COMMITTEE. FROM TIME TO TIME THE BOARD REVIEWS PUBLICLY AVAILABLE DATA FROM OTHER OTHER SIMILAR INSTITUTIONS."}, {"ReturnReference": "FORM 990, PART VI, SECTION C, LINE 19", "Explanation": "THE SCHOOL'S GOVERNING DOCUMENTS AND FINANCIAL STATEMENTS ARE MADE AVAILABLE TO THE PUBLIC UPON REQUEST."}, {"Identifier": "CHANGES IN NET ASSETS OR FUND BALANCES:", "ReturnReference": "FORM 990, PART XI, LINE 5:", "Explanation": "NET UNREALIZED GAINS ON INVESTMENTS: 1,149,801."}, {"Explanation": "THE SCHOOL HAS A FINANCE COMMITTEE THAT ALSO FUNCTIONS AS AN AUDIT COMMITTEE REPORTING TO THE BOARD. THE AUDIT COMMITTEE AND BOARD OF TRUSTEES SELECT THE INDEPENDENT AUDITOR. THE PROCESS HAS NOT CHANGED FROM THE PRIOR YEAR."}]}, "IRS990@documentId": "RetDoc1038000001", "IRS990ScheduleA@documentId": "RetDoc1039100001", "IRS990ScheduleB@documentId": "RetDoc1234500001", "IRS990ScheduleD@documentId": "RetDoc1040000001", "IRS990ScheduleE@documentId": "RetDoc1040400001", "IRS990ScheduleG@documentId": "RetDoc1041300001", "IRS990ScheduleI@documentId": "RetDoc1041900001", "IRS990ScheduleJ@documentId": "RetDoc1042400001", "IRS990ScheduleO@documentId": "RetDoc1044400001"}, "ReturnHeader@binaryAttachmentCount": "0", "ReturnData@documentCount": "9"}, "Return@returnVersion": "2010v3.4"}